<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.backoders.bankodemia">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bankodemia">
        <activity
            android:name=".ui.singup.view.NewAccountActivity"
            android:exported="false" />
        <activity
            android:name=".ui.login.view.LoginActivity"
            android:exported="false" />
        <activity
            android:name="mx.backoders.bankodemia.ui.transactions.view.SendEdit"
            android:exported="false" />
        <activity
            android:name="mx.backoders.bankodemia.ui.main.transactions.view.SendListUsers"
            android:exported="false" />
        <activity
            android:name="mx.backoders.bankodemia.ui.login.view.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>